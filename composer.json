{
  "name": "Unga Rorelsehindrade",
  "type": "project",
  "description": "Main WordPress setup for WordPress sites at Unga RÃ¶relsehindrade",
  "license": "MIT",
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "wordpress/wordpress",
        "type": "webroot",
        "version": "4.0.0",
        "dist": {
          "type": "zip",
          "url": "https://sv.wordpress.org/wordpress-4.0-sv_SE.zip"
        },
        "require": {
          "fancyguy/webroot-installer": "1.1.0"
        }
      }
    },
    {
      "type":"package",
      "package": {
        "name": "wp-papi/papi",
        "version":"1.0.0",
        "source": {
          "url": "https://github.com/wp-papi/papi.git",
          "type": "git",
          "reference":"1.0.0"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "composer/installers": "v1.0.12",
    "wordpress/wordpress": "4.0.0",
    "fancyguy/webroot-installer": "1.1.0",
    "wp-papi/papi": "1.0.0"
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "installer-paths": {
      "web/lib/papi": ["wp-papi/papi"],
      "web/vendor/plugins/{$name}/": ["type:wordpress-plugin"]
    },
    "webroot-dir": "web/wp",
    "webroot-package": "wordpress/wordpress"
  }
}
